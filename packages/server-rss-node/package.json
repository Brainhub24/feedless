{
  "name": "rich-node-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist && ts-node prisma/clean.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "seed": "ts-node prisma/clean.ts && ts-node prisma/seed.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:migrate:reset": "prisma migrate reset -f",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "postinstall": "test -n \"$IGNORE_SCRIPTS\" || yarn prisma generate",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^7.6.15",
    "@prisma/client": "^2.25.0",
    "@rss-proxy/core": "^0.1.0",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^2.25.1",
    "apollo-server-fastify": "^2.25.1",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.6",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "graphql-scalars": "^1.10.0",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "mozilla-readability": "git+https://github.com/mozilla/readability.git",
    "node-fetch": "^2.6.1",
    "prisma": "^2.25.0",
    "puppeteer": "^10.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-block-resources": "^2.3.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.14.4",
    "v8-sandbox": "^2.2.2",
    "xmldom": "^0.6.0",
    "xpath-ts": "^1.3.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/typescript": "2.1.0",
    "@graphql-codegen/typescript-resolvers": "2.1.0",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.13",
    "@types/lodash": "^4.14.172",
    "@types/mozilla-readability": "^0.1.3",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.10",
    "@types/supertest": "^2.0.10",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
